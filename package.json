{
  "displayName": "Homebridge Tesvor",
  "name": "homebridge-tesvor",
  "version": "1.0.3",
  "author": "Marcel Kordek",
  "description": "Homebridge Plugin for Tesvor Robot Vacuum",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/marcelkordek/homebridge-tesvor.git"
  },
  "bugs": {
    "url": "https://github.com/marcelkordek/homebridge-tesvor/issues"
  },
  "homepage": "https://github.com/marcelkordek/homebridge-tesvor#readme",
  "funding": {
    "type": "paypal",
    "url": "https://www.paypal.me/marcelkorrdek/EUR"
  },
  "engines": {
    "node": ">=10.17.0",
    "homebridge": ">=1.3.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "push": "git push",
    "push:version": "npm version patch && git push",
    "publish:patch": "npm version patch && git push && npm publish",
    "publish:minor": "npm version minor && git push && npm publish",
    "publish:major": "npm version major && git push && npm publish",
    "lint": "eslint src/**.ts --max-warnings=0",
    "lint:fix": "eslint --fix src/**.ts --max-warnings=0",
    "watch": "npm run build && npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "keywords": [
    "homebridge-plugin",
    "homebridge-tesvor",
    "tesvor",
    "robot vacuum"
  ],
  "dependencies": {
    "aws-iot-device-sdk": "^2.2.6",
    "aws-sdk": "^2.863.0",
    "crypto": "^1.0.1",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "auto-changelog": "^2.2.1",
    "eslint": "^7.21.0",
    "homebridge": "^1.3.1",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  }
}
